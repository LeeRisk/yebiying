<h1>Listing fms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Format</th>
      <th>Storage</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fms.each do |fm| %>
      <tr>
        <td><%= fm.name %></td>
        <td><%= fm.description %></td>
        <td><%= fm.format %></td>
        <td><%= fm.storage %></td>
        <td><%= link_to 'Show', fm %></td>
        <td><%= link_to 'Edit', edit_fm_path(fm) %></td>
        <td><%= link_to 'Destroy', fm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fm', new_fm_path %>
